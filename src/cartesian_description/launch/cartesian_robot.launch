<launch>
	<param name="robot_description" textfile="$(find cartesian_description)/urdf/cartesian.urdf" />	

	<!-- robot state publisher -->
  	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />

	<!-- joint state publisher with gui -->
	<param name="use_gui" value="true" />
	<node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher"/>

	<!-- Show in Rviz   -->
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find cartesian_description)/config/config.rviz"/>
	<!-- <node name="rviz" pkg="rviz" type="rviz" args=""/> -->
</launch>
